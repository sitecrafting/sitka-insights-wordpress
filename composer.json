{
  "name": "sitecrafting/gearlab-tools-wordpress",
  "description": "Integrate your WordPress site with the GearLab Tools platform",
  "type": "wordpress-plugin",
  "require": {
    "composer/installers": "~1.0",
    "sitecrafting/gearlab-tools-php": "v2.0-alpha.04"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Coby Tamayo",
      "email": "ctamayo@sitecrafting.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "GearLab\\Plugin\\": "src/",
      "GearLab\\WpCli\\": "cli/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GearLab\\Plugin\\": "test/cases/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  }
}
